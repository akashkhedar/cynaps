{
  "name": "cynaps-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cynaps-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/cynaps-e2e/cypress.config.ts",
        "baseUrl": "http://localhost:8080/",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "cynaps:serve:production"
        },
        "ci": {
          "devServerTarget": "cynaps:serve-static"
        }
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["cynaps"]
}


